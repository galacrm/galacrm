{
    "name": "galacrm/galacrm",
    "type": "project",
    "require": {
        "galacrm/security-bundle": "*",
        "symfony/website-skeleton": "^5.2",
        "symfony/flex": "^1.11"
    },
    "require-dev": {
        "symfony/web-profiler-bundle": "^5.2",
        "symfony/debug": "^4.4"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "endpoint": "http://recipe.galacrm.store/",
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "config": {
        "secure-http": false
    },
    "repositories": [
        {
            "type": "svn",
            "url": "git@github.com:galacrm/security-bundle.git"
        }
    ],
    "authors": [
        {
            "name": "Alexandr Zemlyanoy",
            "email": "zemlyanoi.alex@yandex.ru"
        }
    ]
}
